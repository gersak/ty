{
  "name": "@gersak/ty",
  "version": "0.1.2",
  "description": "Modern Web Components Library built with ClojureScript - Zero Dependencies, Semantic Design System, Advanced Modular Architecture. Available as modular components or all-in-one bundle.",
  "main": "ty.js",
  "module": "ty.js",
  "types": "ty.d.ts",
  "files": [
    "*.js",
    "*.d.ts",
    "css/*.css",
    "README.md",
    "LICENSE",
    "BUNDLE_SIZES.md"
  ],
  "exports": {
    ".": {
      "import": "./ty.js",
      "require": "./ty.js",
      "types": "./ty.d.ts"
    },
    "./ty": "./ty.js",
    "./ty-calendar-month": "./ty-calendar-month.js",
    "./ty-calendar": "./ty-calendar.js",
    "./ty-date-picker": "./ty-date-picker.js",
    "./ty-dropdown": "./ty-dropdown.js",
    "./ty-multiselect": "./ty-multiselect.js",
    "./bundle": "./ty.bundle.js",
    "./css": "./css/ty.css",
    "./css/ty.css": "./css/ty.css",
    "./css/*": "./css/*"
  },
  "jsdelivr": "./ty.js",
  "unpkg": "./ty.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/ty-components.git"
  },
  "keywords": [
    "web-components",
    "clojurescript", 
    "ui-library",
    "design-system",
    "modular",
    "zero-dependencies",
    "semantic-css",
    "custom-elements",
    "shadow-dom",
    "modern-web",
    "calendar",
    "dropdown",
    "forms",
    "multiselect"
  ],
  "author": "Your Name <your.email@domain.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/ty-components/issues"
  },
  "homepage": "https://github.com/your-username/ty-components#readme",
  "peerDependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
