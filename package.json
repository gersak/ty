{
  "name": "@gersak/ty",
  "version": "0.1.5",
  "description": "Modern Web Components Library built with ClojureScript - Zero dependency, modular web components",
  "main": "dist/ty.js",
  "module": "dist/ty.js",
  "types": "dist/ty.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ty.js",
      "require": "./dist/ty.js"
    },
    "./bundle": {
      "import": "./dist/ty.bundle.js"
    },
    "./calendar": {
      "import": "./dist/ty-calendar.js"
    },
    "./calendar-month": {
      "import": "./dist/ty-calendar-month.js"
    },
    "./date-picker": {
      "import": "./dist/ty-date-picker.js"
    },
    "./dropdown": {
      "import": "./dist/ty-dropdown.js"
    },
    "./multiselect": {
      "import": "./dist/ty-multiselect.js"
    },
    "./css": {
      "import": "./dev/css/ty.css"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "web-components",
    "clojurescript",
    "ui-components",
    "modular",
    "zero-dependency",
    "custom-elements",
    "shadow-dom"
  ],
  "author": "Gersak <dev@gersak.io>",
  "homepage": "https://github.com/gersak/ty#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gersak/ty.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "shadow-cljs": "^2.28.0",
    "tailwindcss": "4.1"
  },
  "scripts": {
    "dev": "shadow-cljs -A:dev watch site",
    "dev:css": "npx @tailwindcss/cli -i ./site/tailwind.css -o ./dev/css/app.css --watch",
    "release": "shadow-cljs release lib",
    "prepublishOnly": "npm run build:cdn"
  }
}
